<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.triwahyuprasetyo.sqliteservice.MainActivity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <Button
            android:id="@+id/button_insert_mainActivity"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Insert" />

        <Button
            android:id="@+id/button_getAllAnggota_mainActivity"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Get All Anggota" />

        <Button
            android:id="@+id/button_getAnggotaById_mainActivity"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Get Anggota By Id" />

        <Button
            android:id="@+id/button_update_mainActivity"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Update" />

        <Button
            android:id="@+id/button_delete_mainActivity"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Delete" />

        <Button
            android:id="@+id/button_notification_mainActivity"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Notification" />

        <Button
            android:id="@+id/button_serviceStart_mainActivity"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Start Service" />

        <Button
            android:text="Stop Service"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/button_serviceStop_mainActivity" />
    </LinearLayout>
</android.support.constraint.ConstraintLayout>
